@using System.Configuration

@{
    Layout = "~/Views/Shared/_LayoutAuthenticated.cshtml";
    ViewBag.ApplicationUrl = ConfigurationManager.AppSettings["applicationUrl"].ToString();
    ViewBag.TemplateUrl = ConfigurationManager.AppSettings["templateUrl"].ToString();
}


@section styles {

    <link href="@ViewBag.ApplicationUrl/assets/admin/css/views/Dashboard/DashboardIndexStyle.css" rel="stylesheet" type="text/css" />
}

@section scripts {

    <script src="@ViewBag.ApplicationUrl/assets/admin/js/views/Dashboard/DashboardIndexScript.js" type="text/javascript"></script>

    <script>
        jQuery(document).ready(function () {
            DashboardIndexScript.init();
        });
    </script>

}

<div class="row">
    <div class="col-md-12">
        
        Go Rodrigo !

        <input type="button" value="GET" onclick="clickGET();" />
        <input type="button" value="POST" onclick="clickPOST();" />
        <input type="button" value="Save" onclick="clickSave();" />

    </div>
</div>