@using System.Configuration

@{
    ViewBag.ApplicationUrl = ConfigurationManager.AppSettings["applicationUrl"].ToString();
    ViewBag.TemplateUrl = ConfigurationManager.AppSettings["templateUrl"].ToString();
    ViewBag.AppWebApiUrl = ConfigurationManager.AppSettings["appWebApiUrl"].ToString();
    ViewBag.IdentityWebApiUrl = ConfigurationManager.AppSettings["identityWebApiUrl"].ToString();
}

<!DOCTYPE html>
<!--
Template Name: Metronic - Responsive Admin Dashboard Template build with Twitter Bootstrap 3.3.2
